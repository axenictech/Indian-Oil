<div>
  <ul class="breadcrumb">
    <li>
      <a href="#">Home</a>
    </li>
    <li>
      <a href="#"> Manage Activity</a>
    </li>
  </ul>
</div>
<div class="row">
  <div class="box col-md-12">
    <div class="box-inner">
      <div class="box-header well" data-original-title="">
        <h2><i class="glyphicon glyphicon-user"></i>Upload new Activities </h2>

        <div class="box-icon">
          <a href="#" class="btn btn-minimize btn-round btn-default">
	    <i class="glyphicon glyphicon-chevron-up"></i>
	  </a>
          <a href="#" class="btn btn-close btn-round btn-default">
	    <i class="glyphicon glyphicon-remove"></i>
	  </a>
        </div>
      </div>



<div  class="box-content">
 <table class="table table-striped table-bordered table-responsive">
          <thead>
            <tr>
              <th>Id.</th>
              <th>Type</th>
              <th>Description</th>
              <th>Precedence</th>
              <th>Duration</th>                            
              <th>User</th>
             </tr>
          </thead>
          <tbody>
      <%- @activities.each do |activity| %>
        <tr>
          <td style="color: black;"><%=activity.id%></td>
          <td style="color: black;"><%=activity.activity_type%></td>
          <td style="color: black;"><%=activity.name%></td>
          <td style="color: black;"><%=activity.previous_activity%></td>
          <td style="color: black;"><%=activity.duration%></td>
          <td style="color: black;"><%=activity.user_name%></td>
        </tr>
      <%- end %>
          </tbody>
        </table>
</div>
</div>
</div>
  <!--/span-->

</div><!--/row-->